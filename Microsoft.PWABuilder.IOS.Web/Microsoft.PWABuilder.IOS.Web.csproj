<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Resources\ios-project-src\**" />
    <Content Remove="Resources\ios-project-src\**" />
    <EmbeddedResource Remove="Resources\ios-project-src\**" />
    <None Remove="Resources\ios-project-src\**" />
  </ItemGroup>

  <ItemGroup>
